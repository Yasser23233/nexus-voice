# Copy this file to `.env` and fill in your own values before running the server.

PORT=3000

## ICE server configuration
# Public STUN server used by default
STUN_URL=stun:stun.l.google.com:19302

# TURN server credentials (uncomment and configure if you have a TURN server)
#TURN_URL=turn:turn.example.com:3478
#TURN_USER=username
#TURN_PASS=secret

## Force all WebRTC traffic to go through the TURN server
# When set to true the iceTransportPolicy will be set to `relay`. When false
# WebRTC will also attempt direct peer-to-peer connections via STUN
RELAY_ONLY=false